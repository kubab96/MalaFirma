@model PrzewodnikPracyVM


<form method="post">
    <div class="container p-3">
        <div class="card shadow mb-4" style="margin:15px">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Tworzenie operacji</h6>
            </div>
            <div class="card-body">
                <div class="mb-3">
                    <label asp-for="Operacja.TrescOperacji"></label>
                    <input asp-for="Operacja.TrescOperacji" class="form-control" />
                </div>
                <button type="submit" class="btn btn-primary" style="width:150px">Dodaj</button>
            </div>
        </div>
    </div>
</form>

