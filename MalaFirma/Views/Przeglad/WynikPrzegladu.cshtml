@model PrzegladVM

<div class="container p-3">

    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Wynik przeglądu</h6>
        </div>

        <div class="card-body">
        <form method="post">
            <div class="mb-3">
                <label asp-for="Przeglad.WynikPrzegladu"></label>
                <input asp-for="Przeglad.WynikPrzegladu" class="form-control" />
            </div>
            <button type="submit" class="btn btn-primary">Zakończ przegląd</button>
        </form>

    </div>
</div>