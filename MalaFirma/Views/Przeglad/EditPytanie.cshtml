@model Pytanie

<div class="container p-3">
        <div class="card shadow mb-4" style="margin:15px">
            <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Przegląd zamówieia</h6>
        </div>

        <div class="card-body">
            <form method="post">
                <div class="mb-3">
                    <label asp-for="Nazwa"></label>
                    <input asp-for="Nazwa" class="form-control" />
                </div>
                <button type="submit" class="btn btn-info" style="margin-right:5px; float:right;">Edytuj</button>
                <a asp-controller="Przeglad" asp-action="Pytanie" class="btn btn-primary">
                    <i class="bi bi-arrow-90deg-left"></i> Powrót
                </a>

            </form>
        </div>
    </div>
</div>

@section Scripts{
    @{
    <partial name="_ValidationScriptsPartial" />
    }
}